{
	"env": {
		"test": {
			"plugins": [
				["transform-object-rest-spread"],
				["babel-plugin-transform-require-ignore", {
					"extensions": [".less", ".css"]
				}],
			]
		},
		"development": {
			"plugins": ["lodash"]
		},
		"production": {
			"plugins": [
				["transform-object-rest-spread"],
				["babel-plugin-transform-require-ignore", {
					"extensions": [".less", ".css"]
				}],
				["react-peek/babel", {
					"minifyStatics": true
				}],
				"lodash"
			]
		}
	},
	"presets": [
		["env", {
			"targets": {
				"browsers": ["last 2 versions"]
			}
		}],
		"react"
	]
}
