{
	"env": {
		"test": {
			"plugins": []
		},
		"development": {
			"plugins": ["lodash"]
		},
		"production": {
			"plugins": [
				["babel-plugin-transform-require-ignore", {
					"extensions": [".less", ".css"]
				}],
				["react-peek/babel", {
					"minifyStatics": true
				}],
				"lodash"
			]
		}
	},
	"presets": [
		"stage-2",
		"es2015",
		"react"
	]
}
