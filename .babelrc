{
	"env": {
		"test": {
			"plugins": [
				["babel-plugin-transform-require-ignore", {
					"extensions": [".less", ".css"]
				}],
			],
			"presets": [
				"stage-2",
				"es2015",
				"react"
			]
		},
		"development": {
			"plugins": ["lodash"],
			"presets": [
				"stage-2",
				"es2015",
				"react"
			]
		},
		"production": {
			"plugins": [
				["babel-plugin-transform-require-ignore", {
					"extensions": [".less", ".css"]
				}],
				["react-peek/babel", {
					"minifyStatics": true
				}],
				"lodash"
			],
			"presets": [
				"stage-2",
				"es2015",
				"react"
			]
		},
		"modules": {
			"plugins": [
				["transform-object-rest-spread"],
				["babel-plugin-transform-require-ignore", {
					"extensions": [".less", ".css"]
				}],
				["react-peek/babel", {
					"minifyStatics": true
				}],
			],
			"presets": [
				["env", {
					"targets": {
						"browsers": ["last 2 versions"]
					},
					"modules": false
				}],
				"react"
			]
		}
	}
}
